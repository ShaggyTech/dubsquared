{
  "name": "dubsquared.com",
  "version": "0.1.0",
  "description": "Dubsquared - Volkswagen and Audi Specialists located in Arlington, TX",
  "repository": {
    "type": "git",
    "url": "git://github.com/ShaggyTech/dubsquared"
  },
  "author": "Brandon Eichler <ShaggyTech>",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "cloudflare-build": "NITRO_PRESET=cloudflare yarn build",
    "cloudflare-dev": "yarn cloudflare-build && nuxt start",
    "cloudflare-publish-dev": "yarn cloudflare-build && wrangler publish --config wrangler.dev.toml",
    "cloudflare-publish": "yarn cloudflare-build && wrangler publish",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@formkit/nuxt": "1.0.0-beta.11-c95e605",
    "@formkit/themes": "1.0.0-beta.10",
    "@headlessui/vue": "1.6.5",
    "@iconify/json": "2.1.114",
    "@kevinmarrec/nuxt-pwa": "0.4.2",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/svg": "0.4.0",
    "@pinia/nuxt": "0.4.2",
    "@vueuse/core": "9.3.0",
    "@vueuse/motion": "2.0.0-beta.22",
    "@vueuse/nuxt": "9.3.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nuxt": "3.0.0-rc.11",
    "nuxt-windicss": "2.5.2",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "sass-loader": "13.0.2",
    "sitemap": "7.1.1",
    "tailwind-config-viewer": "1.7.2",
    "typescript": "4.8.3",
    "unplugin-icons": "0.14.10",
    "unplugin-vue-components": "0.22.7",
    "vue3-marquee": "3.1.2"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css,scss}": [
      "yarn lintfix"
    ]
  },
  "dependencies": {}
}
